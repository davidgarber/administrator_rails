<%= render('layouts/errors', :object => @project) %>

<%= form_for [@category, @project] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :detail %>
  <%= f.text_field :detail %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <p>
      <%= f.submit %>
  </p>
<% end %>
